{
  "name": "rin-re",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "dev": "react-scripts start",
    "analyze": "source-map-explorer ./build/static/js/main.*",
    "test": "react-scripts test",
    "lint": "eslint . && prettier --list-different \"./**/*.{js,json,css}\"",
    "format": "eslint --fix . && prettier --write \"./**/*.{js,json,css}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "proxy": "https://bangumi.moe",
  "dependencies": {
    "axios": "^0.17.1",
    "humps": "^2.0.1",
    "material-ui": "^1.0.0-beta.21",
    "material-ui-icons": "^1.0.0-beta.17",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.4",
    "mobx-state-tree": "^1.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.17",
    "react-transition-group": "^2.2.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.8.2",
    "source-map-explorer": "^1.5.0"
  }
}
